<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="siloWorkshop" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_shopItem_storage</name>
        <functions>
            <function>$l10n_function_farmSilo</function>
            <function>$l10n_function_workshop</function>
        </functions>
        <image>store/store_storage.dds</image>
        <price>1</price>
        <dailyUpkeep>0</dailyUpkeep>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>RUDOLFHOERMANN</brand>
        <species>PLACEABLE</species>
